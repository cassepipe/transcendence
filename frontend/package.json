{
  "name": "frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "pnpm vite",
    "preview": "pnpm vite preview",
    "build": "pnpm exec vite build",
    "build:watch": "pnpm vite build --watch ",
    "check": "pnpm svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"a11y-click-events-have-key-events:ignore,\"",
    "check:watch": "pnpm svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "pnpm prettier --plugin-search-dir . --check . && eslint .",
    "format": "pnpm prettier --plugin-search-dir . --write .",
    "prepare": "pnpm svelte-kit sync"
  },
  "type": "module",
  "dependencies": {
    "contract": "workspace:*",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@floating-ui/dom": "^1.5.1",
    "@skeletonlabs/skeleton": "^1.11.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.4",
    "@tailwindcss/forms": "^0.5.4",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.32.4",
    "http-proxy": "^1.18.1",
    "postcss": "^8.4.27",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-inspector": "^0.0.3",
    "svelte-reactive-preprocessor": "^0.8.3",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  }
}
