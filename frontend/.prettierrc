{
	"useTabs": true,
	"tabsWidth": 4,
	"singleQuote": false,
	"semi": false,
	"trailingComma": "none",
	"bracketSpacing" : true,
	"bracketSameLine" : false,
	"printWidth": 100,
	"plugins": [
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"pluginSearchDirs": false,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"svelteBracketNewLine": false
}
